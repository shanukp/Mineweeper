{
    "name": "Zynga.Zdk.Services.EOS",
    "file-filters":
        [
            "Assets/Zynga/Zdk/Services/EOS",
            "-Assets/Zynga/Zdk/Services/EOS/Editor/Tests",
            "-Assets/Zynga/Zdk/Services/EOS/Admin/"
        ],
    "dependent-packages":
        [
            "Zynga.Zdk.Services.Common",
            "Zynga.Core.ErrorHandling",
            "Zynga.Core.Platform",
            "Zynga.Core.Tasks"
        ],
    "options":
        [
            {
                "name": "tests",
                "_comments": "Tests-specific code for EOS.",
                "file-filters":
                    [
                        "Assets/Zynga/Zdk/Services/EOS/Editor/Tests"
                    ],
                "dependent-packages":
                    [
                        "NUnit",
                        "NSubstitute"
                    ]
            },
            {
                "name": "admin",
                "_comments": "Adds admin endpoints for EOS",
                "file-filters":
                    [
                        "Assets/Zynga/Zdk/Services/EOS/Admin"
                    ]
            }
        ],
    "base-jenkins-jobs-config": "unitytech-unity-console",
    "jenkins-jobs-config":
        {
            "named-parameter-overrides":
                {
                    "CODE_COVERAGE_PASS_PERCENTAGE": "50"
                }
        },
    "build-configs":
        {
            "default":
                {
                    "min-code-coverage": 45,
                    "required-defines":
                        [
                            "ASSERTIONS_ENABLED"
                        ]
                },
            "dotnet_core":
                {
                    "platforms":
                        [
                            "console"
                        ],
                    "option-sets":
                        [
                            {
                                "name": "tests",
                                "options":
                                    [
                                        "tests",
                                        "admin"
                                    ]
                            }
                        ]
                },
            "unity":
                {
                    "platforms":
                        [
                            "ios",
                            "android",
                            "webgl"
                        ],
                    "min-supported-version": "5.6",
                    "option-sets":
                        [
                            {
                                "name": "tests",
                                "options":
                                    [
                                        "tests",
                                        "admin"
                                    ]
                            }
                        ]
                }
        },
    "metadata":
        {
            "documentation-merger-script":
                [
                    "Assets/Zynga/Zdk/Services/EOS/DocTemplates"
                ]
        }
}
