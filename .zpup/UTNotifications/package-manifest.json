{
    "name": "UTNotifications",
    "file-filters":
    [
        "Assets/UTNotifications/",
        "-Assets/UTNotifications/Resources/UTNotificationsSettings.asset",
        "-Assets/UTNotifications/Sample/",
        "-Assets/UTNotifications/Editor/DemoServer",
        
        "-Assets/UTNotifications/Editor/Android/NativePluginSource",

        "Assets/Plugins/UTNotifications.dll",

        "Assets/Plugins/Android/ShortcutBadger-1.1.21.aar",
        "Assets/Plugins/Android/utnotifications.aar",

        "Assets/Plugins/iOS/UTNotificationsTools.h",
        "Assets/Plugins/iOS/UTNotificationsTools.mm",

        "Assets/Plugins/WSA/UTNotifications/"
    ],
    "dependent-packages":
    [
        "PlayServicesResolver"
    ],
    "metadata":
    {
        "documentation-merger-script":
            [
                "Assets/UTNotifications/DocTemplates"
            ]
    },
    "options":
    [
        {
            "name": "examples",
            "_comments": "Installs sample code and scene provided by UTNotifications.",
            "file-filters":
            [
                "Assets/UTNotifications/Sample/",
                "Assets/UTNotifications/Editor/DemoServer"
            ]
        },
        {
            "name": "source",
            "_comments": "Includes the source files for UTNotifications",
            "file-filters":
            [
                "Assets/UTNotifications/Editor/Android/NativePluginSource"
            ]
        },
        {
            "name": "runResolver",
            "_comments": "Jenkins-specific, consumed by Zynga.Core.Build to run playservices resolver during build",
            "unity-defines":
                [
                    "RUN_PLAY_SERVICES_RESOLVER"
                ]
        },
        {
            "name": "jenkins",
            "_comments": "Jenkins build option.",
            "dependent-options":
                [
                    "runResolver"
                ]
        }
    ]
}
