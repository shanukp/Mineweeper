{
    "name": "Zynga.Zdk.Services.ProfanityFilter",
    "file-filters":
        [
            "Assets/Zynga/Zdk/Services/ProfanityFilter",
            "-Assets/Zynga/Zdk/Services/ProfanityFilter/Editor/Tests",
            "-Assets/Zynga/Zdk/Services/ProfanityFilter/DocTemplates/",
            "-Assets/Zynga/Zdk/Services/ProfanityFilter/IntegrationTests"
        ],
    "dependent-packages":
        [
            "Zynga.Core.ErrorHandling",
            "Zynga.Zdk.Services.Common",
            "Zynga.Core.Util",
            "Zynga.Core.Net",
            "Zynga.Core.Tasks"
        ],
    "options":
        [
            {
                "name": "tests",
                "_comments": "Tests-specific code for the Profanity service.",
                "file-filters":
                    [
                        "Assets/Zynga/Zdk/Services/ProfanityFilter/Editor/Tests/"
                    ],
                "dependent-packages":
                    [
                        "NUnit",
                        "NSubstitute"
                    ]
            },
            {
                "name": "examples",
                "_comments": "Installs documentation template and example code files.",
                "file-filters":
                    [
                        "Assets/Zynga/Zdk/Services/ProfanityFilter/DocTemplates/"
                    ]
            }
        ],
    "build-configs":
        {
            "default":
                {
                    "min-code-coverage": 77,
                    "required-defines": [ "ASSERTIONS_ENABLED" ]
                },
            "dotnet_core":
                {
                    "platforms": [ "console" ],
                    "option-sets":
                    [
                        {
                            "name": "tests",
                            "options":  [ "tests" ]
                        }
                    ]
                },
            "unity":
                {
                    "platforms": [ "ios", "android", "webgl" ],
                    "min-supported-version": "5.6",
                    "option-sets":
                    [
                        {
                            "name": "tests",
                            "options":  [ "tests" ]
                        }
                    ]
                }
        },
    "metadata":
        {
            "documentation-merger-script":
                [
                    "Assets/Zynga/Zdk/Services/ProfanityFilter/DocTemplates"
                ]
        }
}
