{
    "name": "Zynga.Zdk.Services.Media",
    "file-filters":
        [
            "Assets/Zynga/Zdk/Services/Media",
            "-Assets/Zynga/Zdk/Services/Media/IntegrationTests",
            "-Assets/Zynga/Zdk/Services/Media/Editor/Tests/",
            "-Assets/Zynga/Zdk/Services/Media/DocTemplates/"
        ],
    "dependent-packages":
        [
            "Zynga.Core.ErrorHandling",
            "Zynga.Core.Net",
            "Zynga.Core.Util",
            "Zynga.Core.Tasks",
            "Zynga.Zdk.Services.Common"
        ],
    "options":
    [
        {
            "name": "tests",
            "_comments": "Tests-specific code for the Media Service.",
            "file-filters":
            [
                "Assets/Zynga/Zdk/Services/Media/Editor/Tests/"
            ],
            "dependent-packages":
            [
                "NUnit",
                "NSubstitute"
            ]
        },
        {
            "name": "examples",
            "_comments": "Installs documentation template and example code files.",
            "file-filters":
            [
                "Assets/Zynga/Zdk/Services/Media/DocTemplates/"
            ]
        }
    ],
    "metadata":
    {
        "documentation-merger-script":
        [
            "Assets/Zynga/Zdk/Services/Media/DocTemplates/"
        ]
    },
    "build-configs":
    {
        "default":
        {
            "min-code-coverage": 0,
            "required-defines": [ "ASSERTIONS_ENABLED" ]
        },
        "dotnet_core":
        {
            "platforms": [ "console" ],
            "option-sets":
            [
                {
                    "name": "tests",
                    "options":  [ "tests" ]
                }
            ]
        },
        "unity":
        {
            "platforms": [ "ios", "android", "webgl" ],
            "min-supported-version": "5.6",
            "option-sets":
            [
                {
                    "name": "tests",
                    "options":  [ "tests" ]
                }
            ]
        }
    }
}
