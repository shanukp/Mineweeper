{
    "name": "PlayServicesResolver",
    "file-filters":
        [
            "Assets/PlayServicesResolver/"
        ],
    "options":
        [
            {
                "name": "runResolver",
                "_comments": "Jenkins-specific, consumed by Zynga.Core.Build to run playservices resolver during build",
                "unity-defines":
                    [
                        "RUN_PLAY_SERVICES_RESOLVER"
                    ]
            },
            {
                "name": "jenkins",
                "_comments": "Jenkins build option.",
                "dependent-options":
                    [
                        "runResolver"
                    ]
            }
        ],
    "metadata":
    {
        "documentation-merger-script":
        "Assets/PlayServicesResolver/DocTemplates"
    },
    "build-configs": 
    {
        "default": 
        {
            "min-code-coverage": 0,
            "required-defines": [ "ASSERTIONS_ENABLED" ]
        },
        "unity": 
        {
            "platforms": [ "ios", "android", "webgl" ],
            "min-supported-version": "5.6",
            "option-sets": 
            [
                {
                    "name": "none"
                }
            ]
        }
    }

}
