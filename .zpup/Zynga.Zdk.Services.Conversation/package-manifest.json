{
    "name": "Zynga.Zdk.Services.Conversation",
    "file-filters":
        [
            "Assets/Zynga/Zdk/Services/Conversation",
            "-Assets/Zynga/Zdk/Services/Conversation/Editor/Tests",
            "-Assets/Zynga/Zdk/Services/Conversation/Zoom",
            "-Assets/Zynga/Zdk/Services/Conversation/Gateway",
            "-Assets/Zynga/Zdk/Services/Conversation/DocTemplates/",
            "-Assets/Zynga/Zdk/Services/Conversation/IntegrationTests"
        ],
    "dependent-packages":
        [
            "Zynga.Core.Util",
            "Zynga.Core.Net",
            "Zynga.Core.ZLogger",
            "Zynga.Core.Tasks",
            "Zynga.Zdk.Services.Common"
        ]  ,
    "options":
        [
            {
                "name": "tests",
                "_comments": "Tests-specific code for Conversation.",
                "file-filters":
                [
                    "Assets/Zynga/Zdk/Services/Conversation/Editor/Tests"
                ],
                "dependent-packages":
                [
                    "NUnit",
                    "NSubstitute"
                ]
            },
            {
                "name": "use-gateway",
                "_comments": "Gateway integration for the Conversation service.",
                "file-filters":
                [
                    "Assets/Zynga/Zdk/Services/Conversation/Gateway"
                ],
                "dependent-packages":
                [
                    "Zynga.Zdk.Services.Gateway"
                ]
            },
            {
                "name": "use-zoom",
                "_comments": "Zoom integration for the Conversation service.",
                "file-filters":
                [
                    "Assets/Zynga/Zdk/Services/Conversation/Zoom",
                    "-Assets/Zynga/Zdk/Services/Conversation/Zoom/Editor/Tests"
                ],
                "dependent-packages":
                [
                    "Zynga.Zdk.Services.Zoom"
                ]
            },
            {
                "name": "use-zoom-tests",
                "_comments": "Unit tests for zoom integration for the Conversation service.",
                "file-filters":
                [
                    "Assets/Zynga/Zdk/Services/Conversation/Zoom/Editor/Tests"
                ],
                "dependent-options":
                [
                    "use-zoom",
                    "tests"
                ]
            },
            {
                "name": "examples",
                "_comments": "Installs documentation template and example code files.",
                "file-filters":
                    [
                        "Assets/Zynga/Zdk/Services/Conversation/DocTemplates/"
                    ]
            }
        ],
    "metadata":
        {
            "documentation-merger-script": "Assets/Zynga/Zdk/Services/Conversation/DocTemplates"
        },
    "base-jenkins-jobs-config": "unitytech-unity-console",
    "jenkins-jobs-config":
        {
            "named-parameter-overrides":
                {
                    "CODE_COVERAGE_PASS_PERCENTAGE": "50"
                }
        },
    "build-configs":
    {
        "default":
        {
            "min-code-coverage": 50,
            "required-defines": [ "ASSERTIONS_ENABLED" ]
        },
        "dotnet_core":
        {
            "platforms": [ "console" ],
            "option-sets":
            [
                {
                    "name": "tests",
                    "options":  [ "tests" ]
                },
                {
                    "name": "useZoom",
                    "options":  [ "use-zoom", "use-zoom-tests" ]
                }
            ]
        },
        "unity":
        {
            "platforms": [ "ios", "android", "webgl" ],
            "min-supported-version": "5.6",
            "option-sets":
            [
                {
                    "name": "tests",
                    "options":  [ "tests" ]
                },
                {
                    "name": "useZoom",
                    "options":  [ "use-zoom", "use-zoom-tests" ]
                }
            ]
        }
    }
}
