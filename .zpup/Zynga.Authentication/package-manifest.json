{
    "name": "Zynga.Authentication",
    "file-filters":
    [
        "Assets/Zynga/Authentication",
        "-Assets/Zynga/Authentication/Channels",
        "-Assets/Zynga/Authentication/Editor/Tests",
        "-Assets/Zynga/Authentication/IntegrationTests",
        "-Assets/Zynga/Authentication/DocTemplates/",
        "-Assets/Zynga/Authentication/Facebook/",
        "-Assets/Zynga/Authentication/SignInWithApple/"
    ],
    "dependent-packages":
    [
        "Zynga.Core.JsonUtil",
        "Zynga.Core.Util",
        "Zynga.Core.Platform",
        "Zynga.Core.Tasks",
        "Zynga.Core.ErrorHandling",
        "Zynga.Zdk.Services.Common",
        "Zynga.Zdk.Services.Identity",
	"Zynga.Zdk.Services.Messaging",
        "Zynga.Zdk.Services.Track"
    ],
    "options":
    [
        {
            "name": "tests",
            "_comments": "Tests-specific code for the ZIS authentication.",
            "file-filters":
            [
                "Assets/Zynga/Authentication/Editor/Tests"
            ],
            "dependent-packages":
            [
                "NUnit",
                "NSubstitute"
            ]
        },
        {
            "name": "examples",
            "_comments": "Installs documentation template and example code files.",
            "file-filters":
            [
                "Assets/Zynga/Authentication/DocTemplates/"
            ]
        }
    ],
    "metadata":
    {
        "documentation-merger-script":
        [
            "Assets/Zynga/Authentication/DocTemplates"
        ]
    },
    "build-configs":
    {
        "default":
        {
            "min-code-coverage": 60,
            "required-defines": [ "ASSERTIONS_ENABLED" ]
        },
        "dotnet_core":
        {
            "platforms": [ "console" ],
            "option-sets":
            [
                {
                    "name": "tests",
                    "options":  [ "tests" ]
                }
            ]
        },
        "unity":
        {
            "platforms": [ "ios", "android", "webgl" ],
            "min-supported-version": "5.6",
            "option-sets":
            [
                {
                    "name": "testsAndExamples",
                    "options":  [ "tests", "examples" ]
                }
            ]
        }
    }
}
