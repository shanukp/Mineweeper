{
    "name": "Zynga.Metrics.ZTrack",
    "file-filters":
        [
            "Assets/Zynga/Metrics/ZTrack",
            "-Assets/Zynga/Metrics/ZTrack/Editor/Tests",
            "-Assets/Zynga/Metrics/ZTrack/DocTemplates/",
            "-Assets/Zynga/Metrics/ZTrack/IntegrationTests"
        ],
    "dependent-packages":
        [
            "Zynga.Core.ZLogger",
            "Zynga.Core.Util",
            "Zynga.Core.Platform",
            "Zynga.Core.ErrorHandling",
            "Zynga.Core.Tasks",
            "Zynga.Zdk.Services.Track",
            "Zynga.Zdk.Services.Log",
            "Zynga.Zdk.Services.Common",
            "Zynga.Core.Net"
        ],
    "options":
        [
            {
                "name": "tests",
                "_comments": "Unit tests for the package",
                "file-filters":
                    [
                        "Assets/Zynga/Metrics/ZTrack/Editor/Tests",
                    ],
                "dependent-packages":
                    [
                        "NUnit"
                    ]
            }
        ],
    "_comments": "Package depends on unity indirectly for platform defines, and with UnityUtil. Don't create a console version.",
    "metadata":
        {
            "documentation-merger-script":
                [
                    "Assets/Zynga/Metrics/ZTrack/DocTemplates"
                ]
        },
    "build-configs":
        {
            "default":
                {
                    "min-code-coverage": 20,
                    "define-sets":
                        [
                            {
                                "name": "unitTests",
                                "options":
                                    [
                                        "tests"
                                    ]
                            }
                        ],
                    "required-defines":
                        [
                            "ASSERTIONS_ENABLED"
                        ]
                },
            "unity":
                {
                    "platforms":
                        [
                            "ios",
                            "android",
                            "webgl"
                        ],
                    "min-supported-version": "5.6",
                    "option-sets":
                        [
                            {
                                "name": "unitTests",
                                "options":
                                    [
                                        "tests"
                                    ]
                            }
                        ]
                }
        }
}