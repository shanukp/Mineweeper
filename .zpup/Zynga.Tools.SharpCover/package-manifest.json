{
    "name": "Zynga.Tools.SharpCover",
    "_comments": "Instruments assemblies for code coverage, generates XML files useable by ReportGenerator",
    "file-filters":
        [
            "Assets/Zynga/Tools/SharpCover",
            "-Assets/Zynga/Tools/SharpCover/DocTemplates"
        ],
    "dependent-packages":
        [
            "Mono.Cecil"
        ],
    "options":
        [
            {
                "name": "examples",
                "_comments": "Installs documentation templates and examples code files.",
                "file-filters":
                [
                    "Assets/Zynga/Tools/SharpCover/DocTemplates"
                ]
            }
        ],
    "jenkins-jobs-config":
        {
            "reference-job-names":
                [
                    "SharpCoverMacReference",
                    "SharpCoverWindowsReference"
                ]
        },
    "build-configs":
        {
            "default":
                {
                    "min-code-coverage": 0,
                    "required-defines":
                        [
                            "ASSERTIONS_ENABLED"
                        ]
                },
            "unity":
                {
                    "platforms":
                        [
                            "ios",
                            "android",
                            "webgl"
                        ],
                    "min-supported-version": "5.6",
                    "option-sets":
                        [
                            {
                                "name": "none"
                            }
                        ]
                }
        },
    "metadata":
        {
            "documentation-merger-script":
                [
                    "Assets/Zynga/Tools/SharpCover/DocTemplates"
                ]
        }
}
