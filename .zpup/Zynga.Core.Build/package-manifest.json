{
    "name": "Zynga.Core.Build",
    
    "file-filters":
    [
        "Assets/Zynga/Core/Build"
    ],
    "base-jenkins-jobs-config": "unitytech-unity",
    "jenkins-jobs-config":
    {
        "named-parameter-overrides":
        {
            "CODE_COVERAGE_PASS_PERCENTAGE": "0"
        }
    },
    "metadata":
    {
        "documentation-merger-script":
        "Assets/Zynga/Core/Build/DocTemplates"
    },
    "build-configs": 
    {
        "default": 
        {
            "min-code-coverage": 0,
            "required-defines": [ "ASSERTIONS_ENABLED" ]
        },
        "unity": 
        {
            "platforms": [ "ios", "android", "webgl" ],
            "min-supported-version": "5.6",
            "option-sets":
            [
                {
                    "name": "none"
                }
            ]
        }
    }
}
