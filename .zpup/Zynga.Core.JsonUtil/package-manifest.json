{
    "name": "Zynga.Core.JsonUtil",
    "file-filters":
        [
            "Assets/Zynga/Core/JsonUtil",
            "-Assets/Zynga/core/JsonUtil/Editor/Tests",
            "-Assets/Zynga/Core/JsonUtil/DocTemplates"
        ],
    "dependent-packages":
        [
            "Zynga.Core.ErrorHandling"
        ],
    "options":
        [
            {
                "name": "tests",
                "_comments": "Tests-specific code for Core.JsonUtil.",
                "file-filters":
                    [
                        "Assets/Zynga/Core/JsonUtil/Editor/Tests"
                    ],
                "dependent-packages":
                    [
                        "NUnit"
                    ]
            },
            {
                "name": "examples",
                "_comments": "Installs documentation templates and examples code files.",
                "file-filters":
                    [
                        "Assets/Zynga/Core/JsonUtil/DocTemplates"
                    ]
            }
        ],
    "metadata":
        {
            "documentation-merger-script":
                [
                    "Assets/Zynga/Core/JsonUtil/DocTemplates"
                ]
        },
    "build-configs":
        {
            "default":
                {
                    "min-code-coverage": 95,
                    "required-defines":
                        [
                            "ASSERTIONS_ENABLED"
                        ]
                },
            "dotnet_core":
                {
                    "min-code-coverage": 90,
                    "platforms":
                        [
                            "console"
                        ],
                    "option-sets":
                        [
                            {
                                "name": "tests",
                                "options":
                                    [
                                        "tests"
                                    ]
                            }
                        ]
                },
            "unity":
                {
                    "platforms":
                        [
                            "ios",
                            "android",
                            "webgl"
                        ],
                    "min-supported-version": "5.6",
                    "option-sets":
                        [
                            {
                                "name": "testsAndExamples",
                                "options":
                                    [
                                        "tests",
                                        "examples"
                                    ]
                            }
                        ]
                }
        }
}
