{
    "name": "Zynga.Payments.IAP",
    "file-filters":
        [
            "Assets/Zynga/Payments",
            "Assets/Plugins/iOS/ZyngaInAppPurchase.h",
            "Assets/Plugins/iOS/ZyngaInAppPurchase.mm",
            "Assets/Plugins/iOS/ZyngaInAppPurchase.mm.meta",
            "Android/Payments",
            "-Assets/Zynga/Payments/IAP/Editor/Tests",
            "-Assets/Zynga/Payments/IAP/Impl/FacebookStore.cs",
            "-Assets/Zynga/Payments/IAP/Impl/FacebookTransaction.cs",
            "-Assets/Zynga/Payments/IAP/Impl/FacebookTransactionFactory.cs",
            "-Assets/Zynga/Payments/IAP/Impl/FacebookAdapter.cs",
            "-Assets/Zynga/Payments/IAP/Impl/FacebookAdapterBase.cs",
            "-Assets/Zynga/Payments/IAP/Impl/StripeAdapter.cs",
            "-Assets/Zynga/Payments/IAP/Impl/IStripeAdapter.cs",
            "-Assets/Zynga/Payments/IAP/Impl/StripeStore.cs",
            "-Assets/Zynga/Payments/IAP/Impl/StripeTransactionFactory.cs",
            "-Assets/Zynga/Payments/IAP/Impl/Plugins/Stripe",
            "-Assets/WebGLTemplates/BaseTemplate"
        ],
    "dependent-packages":
        [
            "Zynga.Core.Util",
            "Zynga.Core.JsonUtil",
            "Zynga.Core.ZLogger",
            "Zynga.Core.UnityUtil",
            "Zynga.Core.Net",
            "Zynga.Core.Tasks",
            "Zynga.Zdk.Services.Common",
            "Zynga.Zdk.Services.Track",
            "Zynga.Zdk.Services.Log",
            "Zynga.Core.Platform"            
        ],
    "options":
        [
            {
                "name": "tests",
                "_comments": "Tests-specific code for the IAP package.",
                "file-filters":
                    [
                        "Assets/Zynga/Payments/IAP/Editor/Tests",
                        "-Assets/Zynga/Payments/IAP/Editor/Tests/WebGL"
                    ],
                "dependent-packages":
                    [
                        "NUnit",
                        "NSubstitute"
                    ]
            },
            {
                "name": "webgl",
                "_comments": "Includes Web Stores that can be used when targeting Unity WebGL",
                "file-filters":
                [
                    "Assets/Zynga/Payments/IAP/Impl/FacebookStore.cs",
                    "Assets/Zynga/Payments/IAP/Impl/FacebookTransaction.cs",
                    "Assets/Zynga/Payments/IAP/Impl/FacebookTransactionFactory.cs",
                    "Assets/Zynga/Payments/IAP/Impl/FacebookAdapter.cs",
                    "Assets/Zynga/Payments/IAP/Impl/FacebookAdapterBase.cs",
                    "Assets/Zynga/Payments/IAP/Impl/StripeAdapter.cs",
                    "Assets/Zynga/Payments/IAP/Impl/IStripeAdapter.cs",
                    "Assets/Zynga/Payments/IAP/Impl/StripeStore.cs",
                    "Assets/Zynga/Payments/IAP/Impl/StripeTransactionFactory.cs"
                ],
                "dependent-packages":
                [
                    "FacebookUnitySDK",
                    "Zynga.Payments.StripePlugin"
                ]
            },
            {
                "name": "webgl-tests",
                "_comments": "Tests-specific code for the IAP package webgl option.",
                "file-filters":
                [
                    "Assets/Zynga/Payments/IAP/Editor/Tests/WebGL"
                ],
                "dependent-options":
                [
                    "webgl",
                    "tests"
                ]
            }
        ],
    "jenkins-jobs-config":
        {
            "reference-job-names":
                [
                    "UnityTechUnity5XReference-Triggered",
                    "UnityTechUnity53Reference-Triggered",
                    "UnityTechUnity5XReference-Nightly",
                    "UnityTechUnity53Reference-Nightly"
                ],
            "named-parameter-overrides":
                {
                    "CODE_COVERAGE_PASS_PERCENTAGE": "65",
                    "TRIGGERED_5X_TARGET_PLATFORMS": "android,webgl",
                    "TRIGGERED_53_TARGET_PLATFORMS": "ios",
                    "NIGHTLY_5X_TARGET_PLATFORMS": "ios,android,webgl",
                    "NIGHTLY_53_TARGET_PLATFORMS": "ios,android,webgl"
                }
        },
    "build-configs":
        {
            "default":
                {
                    "min-code-coverage": 65,
                    "required-defines":
                        [
                            "ASSERTIONS_ENABLED"
                        ]
                },
            "unity":
                {
                    "platforms":
                        [
                            "ios",
                            "android",
                            "webgl"
                        ],
                    "min-supported-version": "5.6",
                    "option-sets":
                        [
                            {
                                "name": "tests",
                                "options":
                                    [
                                        "tests"
                                    ]
                            },
                            {
                                "name": "webgl-tests",
                                "options":
                                [
                                    "webgl-tests"
                                ]
                            }
                        ]
                }
        },
    "metadata":
        {
            "documentation-merger-script":
                [
                    "Assets/Zynga/Payments/IAP/DocTemplates"
                ]
        }
}
