{
    "name": "Zynga.Core.Net",
    "file-filters":
        [
            "Assets/Zynga/Core/Net",
            "-Assets/Zynga/Core/Net/Unity",
            "-Assets/Zynga/Core/Net/DotNet45",
            "-Assets/Zynga/Core/Net/DotNet45/Editor/Tests",
            "-Assets/Zynga/Core/Net/Editor/Tests",
            "-Assets/Zynga/Core/Net/DocTemplates/"
        ],
    "dependent-packages":
        [
            "Zynga.Core.Util",
            "Zynga.Core.ZLogger",
            "Zynga.Core.Tasks",
            "Zynga.Core.ErrorHandling"
        ],
    "options":
        [
            {
                "name": "unity",
                "_comments": "Unity-specific code for the Net package.",
                "file-filters":
                    [
                        "Assets/Zynga/Core/Net/Unity",
                        "-Assets/Zynga/Core/Net/Unity/Editor/Tests",
                        "-Assets/Zynga/Core/Net/Unity/IntegrationTests",
                        "Assets/Plugins/iOS/ZyngaInternetReachability.h",
                        "Assets/Plugins/iOS/ZyngaInternetReachability.h.meta",
                        "Assets/Plugins/iOS/ZyngaInternetReachability.m",
                        "Assets/Plugins/iOS/ZyngaInternetReachability.m.meta"
                    ],
                "dependent-packages":
                    [
                        "Zynga.Core.UnityUtil",
                        "Zynga.Core.JsonUtil"
                    ]
            },
            {
                "name": "unitytests",
                "_comments": "Unity-tests-specific code for the Net package.",
                "file-filters":
                    [
                        "Assets/Zynga/Core/Net/Unity/Editor/Tests"
                    ],
                "dependent-packages":
                    [
                        "NUnit",
                        "NSubstitute"
                    ],
                "dependent-options":
                    [
                        "unity",
                        "tests"
                    ]
            },
            {
                "name": "dotnet45",
                "_comments": ".NET Framework 4.5+ extensions for the Net package.",
                "file-filters":
                [
                    "Assets/Zynga/Core/Net/DotNet45",
                    "-Assets/Zynga/Core/Net/DotNet45/Editor/Tests"
                ]
            },
            {
                "name": "console",
                "_comments": "When compiling for the console, bring in the dotnet45 option as well.",
                "dependent-options":
                    [
                        "dotnet45"
                    ]
            },
            {
                "name": "tests",
                "_comments": "Tests-specific code for the Net package.",
                "file-filters":
                    [
                        "Assets/Zynga/Core/Net/Editor/Tests"
                    ],
                "dependent-packages":
                    [
                        "NUnit",
                        "NSubstitute"
                    ]
            },
            {
                "name": "dotnet45tests",
                "_comments": "Tests for the .NET Framework 4.5+ extensions.",
                "file-filters":
                    [
                        "Assets/Zynga/Core/Net/DotNet45/Editor/Tests"
                    ],
                "dependent-options":
                    [
                        "tests"
                    ]
            },
            {
                "name": "examples",
                "_comments": "Installs documentation templates and examples code files.",
                "file-filters":
                [
                    "Assets/Zynga/Core/Net/DocTemplates/"
                ]
            }
        ],
    "metadata":
        {
            "documentation-merger-script":
            "Assets/Zynga/Core/Net/DocTemplates"
        },
    "build-configs":
    {
        "default": 
        {
            "min-code-coverage": 95,
            "required-defines": [ "ASSERTIONS_ENABLED" ]
        },
        "dotnet_core": 
        {
            "min-code-coverage": 95,
            "platforms": [ "console" ],
            "option-sets": 
            [
                {
                    "name": "tests",
                    "options":  [ "dotnet45tests" ]
                }
            ]
        },
        "unity": 
        {
            "platforms": [ "ios", "android", "webgl" ],
            "min-supported-version": "5.6",
            "option-sets": 
            [
                {
                    "name": "testsAndExamples",
                    "options": [ "unitytests", "examples" ]
                }
            ]
        }
    }
}
