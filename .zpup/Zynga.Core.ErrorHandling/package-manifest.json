{
    "name": "Zynga.Core.ErrorHandling",
    "file-filters":
        [
            "Assets/Zynga/Core/ErrorHandling",
            "-Assets/Zynga/Core/ErrorHandling/Editor/Tests",
            "-Assets/Zynga/Core/ErrorHandling/DocTemplates/"
        ],
    "dependent-packages":
        [
            "Zynga.Core.ZLogger"
        ],
    "options":
    [
        {
            "name": "examples",
            "_comments": "Installs documentation templates and examples code files.",
            "file-filters":
            [
                "Assets/Zynga/Core/ErrorHandling/DocTemplates/"
            ]
        },
        {
            "name": "tests",
            "_comments": "Tests-specific code for Core.ErrorHandling.",
            "file-filters":
            [
                "Assets/Zynga/Core/ErrorHandling/Editor/Tests"
            ],
            "dependent-packages":
            [
                "NUnit"
            ]
        }
    ],
    "metadata":
    {
        "documentation-merger-script":
        "Assets/Zynga/Core/ErrorHandling/DocTemplates"
    },
    "build-configs":
    {
        "default":
        {
            "min-code-coverage": 95,
            "define-sets":
            [
                {
                    "name": "none",
                    "defines": [ ]
                },
                {
                    "name": "preconditionsMessageBox",
                    "defines": [ "PRECONDITIONS_MESSAGEBOX" ]
                }
            ]
        },
        "dotnet_core":
        {
            "platforms": [ "console" ],
            "option-sets":
            [
                {
                    "name": "tests",
                    "options":  [ "tests" ]
                }
            ]
        },
        "unity":
        {
            "platforms": [ "ios", "android", "webgl" ],
            "min-supported-version": "5.6",
            "option-sets":
            [
                {
                    "name": "testsAndExamples",
                    "options":  [ "tests", "examples" ]
                }
            ]
        }
    }
}
