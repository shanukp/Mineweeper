{
    "name": "Zynga.Zdk.Services.Finder",
    
    "file-filters":
        [
            "Assets/Zynga/Zdk/Services/Finder"
        ],

    "dependent-packages":
        [
            "Zynga.Core.Util",
            "Zynga.Core.JsonUtil",
            "Zynga.Core.ZLogger",
            "Zynga.Core.Net",
            "Zynga.Core.Tasks",
            "Zynga.Zdk.Services.Common"
        ],
    "options":
        [
        ],
    "base-jenkins-jobs-config": "unitytech-unity-console",
    "jenkins-jobs-config":
        {
            "named-parameter-overrides":
                {
                    "CODE_COVERAGE_PASS_PERCENTAGE": "0"
                }
        },
    "metadata":
        {
            "documentation-merger-script":
                [
                    "Assets/Zynga/Zdk/Services/Finder/DocTemplates"
                ]
        },
    "build-configs": 
    {
        "default": 
        {
            "min-code-coverage": 0,
            "required-defines": [ "ASSERTIONS_ENABLED" ]
        },
        "dotnet_core": 
        {
            "platforms": [ "console" ],
            "option-sets": 
            [
                {
                    "name": "tests",
                    "options":  [ "tests" ]
                }
            ]
        },
        "unity": 
        {
            "platforms": [ "ios", "android", "webgl" ],
            "min-supported-version": "5.0",
            "option-sets": 
            [
                {
                    "name": "tests",
                    "options":  [ "tests" ]
                }
            ]
        }
    }
}
