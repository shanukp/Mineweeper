{
    "name": "Zynga.Core.WebSocket",
    "_comments": "WebSocket abstraction layer",
    "file-filters":
        [
            "Assets/Zynga/Core/WebSocket",
            "Assets/Zynga/Core/WebSocket/WebSocketSharp/Plugins/websocket-sharp.dll.meta",
            "-Assets/Zynga/Core/WebSocket/DocTemplates",
            "-Assets/Zynga/Core/WebSocket/Editor/Tests"
        ],
    "dependent-packages":
        [
            "Zynga.Core.Util",
            "Zynga.Core.UnityUtil",
            "Zynga.Core.JsonUtil",
            "Zynga.Core.ZLogger"
        ],
    "options":
        [
            {
                "name": "tests",
                "_comments": "Tests-specific code for the Zynga.Core.WebSocket package",
                "file-filters":
                    [
		        "Assets/Zynga/Core/WebSocket/Editor/Tests"
                    ],
                "dependent-packages":
                    [
                        "Zynga.Core.Tasks",
                        "NUnit",
                        "NSubstitute"
                    ]
            },
            {
                "name": "examples",
                "_comments": "Examples of using Zynga.Core.WebSocket",
                "file-filters":
                    [
                        "Assets/Zynga/Core/WebSocket/DocTemplates"
                    ]
            }
        ],
    "metadata":
        {
            "documentation-merger-script": "Assets/Zynga/Core/WebSocket/DocTemplates"
        },
    "build-configs":
        {
            "default":
                {
                    "min-code-coverage": 80,
                    "required-defines":
                        [
                            "ASSERTIONS_ENABLED"
                        ]
                },
            "unity":
                {
                    "platforms":
                        [
                            "ios",
                            "android",
                            "webgl"
                        ],
                    "min-supported-version": "5.6",
                    "option-sets":
                        [
                            {
                                "name": "tests",
                                "options":
                                    [
                                        "tests" 
                                    ]
                            },
                            {
                                "name": "examples",
                                "options":
                                    [
                                        "examples"
                                    ]
                            }
                        ]
                }
        }
}
