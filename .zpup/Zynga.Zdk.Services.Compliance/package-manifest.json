{
    "name": "Zynga.Zdk.Services.Compliance",
    "file-filters": [
        "Assets/Zynga/Zdk/Services/Compliance",
        "-Assets/Zynga/Zdk/Services/Compliance/Editor/Tests",
        "-Assets/Zynga/Zdk/Services/Compliance/DocTemplates/",
        "-Assets/Zynga/Zdk/Services/Compliance/IntegrationTests"
    ],
    "dependent-packages": [
        "Zynga.Core.ErrorHandling",
        "Zynga.Core.Net",
        "Zynga.Core.Util",
        "Zynga.Core.JsonUtil",
        "Zynga.Core.Tasks",
        "Zynga.Zdk.Services.Common"
    ],
    "options": [
        {
            "name": "tests",
            "_comments": "Tests-specific code for the Compliance service.",
            "file-filters": [
                "Assets/Zynga/Zdk/Services/Compliance/Editor/Tests/"
            ],
            "dependent-packages": [
                "NUnit",
                "NSubstitute"
            ]
        },
        {
            "name": "examples",
            "_comments": "Installs documentation template and example code files.",
            "file-filters": [
                "Assets/Zynga/Zdk/Services/Compliance/DocTemplates/"
            ]
        }
    ],
    "metadata": {
        "documentation-merger-script": [
            "Assets/Zynga/Zdk/Services/Compliance/DocTemplates"
        ]
    },
    "build-configs": {
        "default": {
            "min-code-coverage": 80,
            "required-defines": [
                "ASSERTIONS_ENABLED"
            ]
        },
        "dotnet_core": {
            "platforms": [
                "console"
            ],
            "option-sets": [
                {
                    "name": "tests",
                    "options": [
                        "tests"
                    ]
                }
            ]
        },
        "unity": {
            "platforms": [
                "ios",
                "android",
                "webgl"
            ],
            "min-supported-version": "5.6",
            "option-sets": [
                {
                    "name": "tests",
                    "options": [
                        "tests"
                    ]
                }
            ]
        }
    }
}