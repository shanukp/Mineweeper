{
    "name": "Zynga.Authentication.GooglePlayGames",
    "file-filters":
    [
        "Assets/Zynga/Authentication/Channels/GooglePlayGames",
        "-Assets/Zynga/Authentication/Channels/GooglePlayGames/Unity",        
        "-Assets/Zynga/Authentication/Channels/GooglePlayGames/Editor/Tests",
        "-Assets/Zynga/Authentication/Channels/GooglePlayGames/DocTemplates/"
    ],
    "dependent-packages":
    [
        "Zynga.Authentication",
        "Zynga.Core.Tasks",
        "Zynga.Core.Platform",
        "Zynga.Core.Util",
        "Zynga.Zdk.Services.Common"
    ],
    "options":
    [
        {
            "name": "unity",
            "_comments": "Unity-specific code for Zynga.Authentication.GooglePlayGames.",
            "file-filters":
            [
                "Assets/Zynga/Authentication/Channels/GooglePlayGames/Unity"
            ],
            "dependent-packages":
            [
                "GooglePlayGames"
            ]
        },
        {
            "name": "tests",
            "_comments": "Tests-specific code for the ZIS GooglePlayGames authentication.",
            "file-filters":
            [
                "Assets/Zynga/Authentication/Channels/GooglePlayGames/Editor/Tests"
            ],
            "dependent-packages":
            [
                "NUnit",
                "NSubstitute"
            ]
        },
        {
            "name": "examples",
            "_comments": "Installs documentation template and example code files.",
            "file-filters":
            [
                "Assets/Zynga/Authentication/Channels/GooglePlayGames/DocTemplates/"
            ]
        }
    ],
    "metadata":
    {
        "documentation-merger-script":
        [
            "Assets/Zynga/Authentication/Channels/GooglePlayGames/DocTemplates"
        ]
    },
    "build-configs":
    {
        "default":
        {
            "min-code-coverage": 0,
            "required-defines": [ "ASSERTIONS_ENABLED" ]
        },
        "dotnet_core":
        {
            "platforms": [ "console" ],
            "option-sets":
            [
                {
                    "name": "tests",
                    "options": [ "jenkins", "tests" ]
                }
            ]
        },
        "unity":
        {
            "platforms": [ "ios", "android", "webgl" ],
            "min-supported-version": "5.6",
            "option-sets":
            [
                {
                    "name": "tests",
                    "options": [ "jenkins", "tests" ]
                }
            ]
        }
    }
}
