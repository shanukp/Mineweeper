{
    "name": "Zynga.Zdk.Services.Identity",
    "file-filters":
    [
        "Assets/Zynga/Zdk/Services/Identity",
        "-Assets/Zynga/Zdk/Services/Identity/IntegrationTests",
        "-Assets/Zynga/Zdk/Services/Identity/Editor/Tests",
        "-Assets/Zynga/Zdk/Services/Identity/DocTemplates/"
    ],
    "dependent-packages":
    [
        "Zynga.Core.JsonUtil",
        "Zynga.Core.Util",
        "Zynga.Core.Net",
        "Zynga.Core.Tasks",
        "Zynga.Core.ZLogger",
        "Zynga.Core.ErrorHandling",
        "Zynga.Zdk.Services.Common"
    ],
    "options":
    [
        {
            "name": "tests",
            "_comments": "Tests-specific code for the zynga Identity service.",
            "file-filters":
            [
                "Assets/Zynga/Zdk/Services/Identity/Editor/Tests"
            ],
            "dependent-packages":
            [
                "NUnit",
                "NSubstitute"
            ]
        },
        {
            "name": "examples",
            "_comments": "Installs documentation template and example code files.",
            "file-filters":
            [
                "Assets/Zynga/Zdk/Services/Identity/DocTemplates/"
            ]
        }
    ],
    "metadata":
    {
        "documentation-merger-script":
        [
            "Assets/Zynga/Zdk/Services/Identity/DocTemplates"
        ]
    },
    "build-configs":
    {
        "default":
        {
            "min-code-coverage": 80,
            "required-defines": [ "ASSERTIONS_ENABLED" ]
        },
        "dotnet_core":
        {
            "platforms": [ "console" ],
            "option-sets":
            [
                {
                    "name": "tests",
                    "options":  [ "tests" ]
                }
            ]
        },
        "unity":
        {
            "platforms": [ "ios", "android", "webgl" ],
            "min-supported-version": "5.6",
            "option-sets":
            [
                {
                    "name": "testsAndExamples",
                    "options":  [ "tests", "examples" ]
                }
            ]
        }
    }
}
