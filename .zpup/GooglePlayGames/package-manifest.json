{
    "name": "GooglePlayGames",
    "file-filters":
        [
            "Assets/GooglePlayGames",
            "-Assets/GooglePlayGames/GameInfo.cs",
            "-Assets/GooglePlayGames/Plugins/Android/GooglePlayGamesManifest.plugin/AndroidManifest.xml",
            "-Assets/GooglePlayGames/Editor/m2repository/com/google/games/gpgs-plugin-support/0.9.58/gpgs-plugin-support-0.9.58.aar"

        ],
    "dependent-packages":
        [
            "PlayServicesResolver"
        ],
    "options":
        [
            {
                "name": "gameSpecificFiles",
                "_comments": "Game specific files required for running, used during package build. Not intended to be consumed by game teams.",
                "file-filters":
                    [
                        "Assets/GooglePlayGames/GameInfo.cs",
                        "Assets/GooglePlayGames/Plugins/Android/GooglePlayGamesManifest.plugin/AndroidManifest.xml"
                    ]
            },
            {
                "name": "runResolver",
                "_comments": "Jenkins-specific, consumed by Zynga.Core.Build to run playservices resolver during build",
                "unity-defines":
                    [
                        "RUN_PLAY_SERVICES_RESOLVER"
                    ]
            },
            {
                "name": "jenkins",
                "_comments": "Jenkins build option.",
                "dependent-options":
                    [
                        "gameSpecificFiles"
                    ]
            }
        ],
    "build-configs":
        {
            "default":
                {
                    "min-code-coverage": 0,
                    "required-defines":
                        [
                            "ASSERTIONS_ENABLED"
                        ]
                },
            "unity":
                {
                    "platforms":
                        [
                            "ios",
                            "android",
                            "webgl"
                        ],
                    "min-supported-version": "5.6",
                    "option-sets":
                        [
                            {
                                "name": "base",
                                "options":
                                    [
                                        "jenkins"
                                    ]
                            }
                        ]
                }
        },
    "metadata":
        {
            "documentation-merger-script":
                [
                    "Assets/GooglePlayGames/DocTemplates"
                ]
        }
}
